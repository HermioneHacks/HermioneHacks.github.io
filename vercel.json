{
  "version": 2,
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    { "src": "/_vercel/insights/(.*)", "dest": "/_vercel/insights/$1" },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "functions": {
    "api/config.js": {
      "runtime": "nodejs18.x"
    }
  }
}